class Solution
{
    public:
    //Function to return Breadth First Traversal of given graph.
	vector<int>bfsOfGraph(int V, vector<int> adj[])
	{
	    // Code here
	    vector<int>bfs;
	    vector<bool>visited(V,false);
	    
	    int start=0;
	    visited[start]=true;
	    

	            queue<int>q;
	            q.push(start);
	
	            while(!q.empty())
	            {
	                int node=q.front();
	                bfs.push_back(node);
	                q.pop();
	                
	                
	                for(auto it:adj[node])
	                {
	                    if(!visited[it]){
	                    q.push(it);
	                    visited[it]=true;
	                }
	                }
	    }
	    return bfs;
	}
};
