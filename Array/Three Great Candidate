Approach-1(Using Brute Force)
T.C.-O(N^3)
S.C.-O(
class Solution{   
public:
    long long maxProduct(int arr[], int n) {
        // code here
        
        
        int res=INT_MIN;
        
        for(int i=0;i<n-2;i++)
        {
            for(int j=i+1;j<n-1;j++)
            {
                for(int k=j+1;k<n;k++)
                {
                    res=max(res,arr[i]*arr[j]*arr[k]);
                }
            }
        }
        return res;
    }
};
